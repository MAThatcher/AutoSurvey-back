<!DOCTYPE html>
<html xmlns:ng="http://angularjs.org" prefix="og: http://ogp.me/ns#" id="ng-app" lang="en" class="no-js top-navbar-xlarge ls-bottom-footer" ng-app="webApp" ng-cloak>

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, user-scalable=no">
    <meta name="fragment" content="!">
    <meta name="description" content="{{metaDescription}}">
    <meta name="keywords" content="{{metaTags}}">
    <meta property="fb:app_id" content="{{fbAppId}}" />
    <meta name="google-site-verification" content="2aMgq0lCGtrTeQn0ksNMrdoQoGHTxDOQiY8tDpIT3Jo">
    <meta http-equiv="cache-control" content="max-age=0">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="0">
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT">
    <meta http-equiv="pragma" content="no-cache">
    <meta property="og:title" content="{{appTitle}}" />
    <meta property="og:description" content="{{metaDescription}}" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="{{slugUrl}}" />
    <meta property="og:site_name" content="Revature" />
    <meta property="og:image" content="{{slugImage}}" />
    <meta property="og:image:secure_url" content="{{slugImage}}" />
    <meta property="og:image:type" content="image/jpg" />
    <meta property="og:image:width" content="1280" />
    <meta property="og:image:height" content="720" id="ogImageHeight" />

    <base href="/">

    <title ng-bind="appTitle">RevaturePro</title>

    <link rel="icon" ng-href={{faviconUrl}}>
    <!-- assets/css/vendor/bundle.8a213c6e.css -->
    <link rel="stylesheet" href="assets/css/vendor/bundle.8a213c6e.css">

    <script type="text/javascript" src="https://use.typekit.net/pvo4jdm.js"></script>
    <!-- Underscore JS -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>

    <!-- for gPlus login -->
    <script type="text/javascript" src="https://apis.google.com/js/platform.js"></script>
    <!-- for fb login -->
    <script type="text/javascript" src="https://connect.facebook.net/en_US/sdk.js"></script>

    <!-- assets/js/bundle.44dd8978.js -->
    <script src="assets/js/bundle.44dd8978.js"></script>
    <script type="text/javascript" src="assets/lib/ckeditor/ckeditor.js"></script>

    <!-- Image viewer script -->
    <script type="text/javascript" src="assets/lib/ImageViewer/iv-viewer.min.js"></script>

    <!-- Quiz Matching type drop down scripts -->
    <script type="text/javascript" src="assets/js/app/jquery.selectBoxIt.min.js"></script>

    <script type="text/javascript">
        (function(i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r;
            i[r] = i[r] || function() {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date();
            a = s.createElement(o),
                m = s.getElementsByTagName(o)[0];
            a.async = 1;
            a.src = g;
            m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        try {
            Typekit.load({
                async: true
            });
        } catch (e) {}

        function doc_keyUp(e) {
            if (e.ctrlKey && e.keyCode == 38) {
                $("html, body").animate({
                    scrollTop: 0
                }, 600);
            }
        }
        document.addEventListener('keyup', doc_keyUp, false);

        $(document).ready(function() {
            $(window).scroll(function() {
                if ($(this).scrollTop() > 100) {
                    $('.scrollup').fadeIn();
                } else {
                    $('.scrollup').fadeOut();
                }
            });

            $('.scrollup').click(function() {
                $("html, body").animate({
                    scrollTop: 0
                }, 600);
                return false;
            });

            // for browser back button
            var loaded = false;
            window.onpopstate = function(e) {
                if (!loaded) {
                    loaded = true;
                    $("div").removeClass("modal-backdrop fade");
                    $(".modal-backdrop").removeClass("in");
                }
            }

            // For magnifying image from ckeditor
            const viewer = new ImageViewer.FullScreenViewer();
            $(document).on("click", ".ckEditorStyles img, .gitLabStyles img", function() {
                viewer.show(this.src, this.src);
            });

        });
    </script>
</head>

<body data-spy="scroll" class="{{bodyClass}} mob-pad-50 breakpoint-1024 custom-theme">

    <div id="fb-root"></div>

    <div class="ajax-loader" id="main-ajax-loader">
        <img src="assets/images/ajax-loader-logo.0cd555cc.gif" alt="Application Loading">
    </div>

    <div class="ajax-loader" id="ajax-loader">
        <img src="assets/images/ajax-loader.3a0a4d71.gif" alt="View Loading">
    </div>

    <div class="fixed-header" ng-include src="'app/modules/common/views/common-header.a2249b1b.html'"></div>

    <div ng-view></div>
    <a href="#" class="scrollup">Scroll</a>

    <footer ng-include src="'app/modules/common/views/common-footer.b909df87.html'"></footer>

    <!-- Modal -->
    <div class="modal grow modal-backdrop-white fade" id="internetConnect" data-backdrop="static" data-keyboard="false" tabindex="-1" style="overflow: hidden;">
        <div class="modal-dialog modal-md">
            <div class="v-cell">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">
                            <i class="fa fa-exclamation-triangle"></i> Not Connected
                        </h4>
                    </div>
                    <div class="modal-body">
                        <p>We're having trouble connecting to our servers. Please check your internet connection. This message will automatically go away once connectivity is restored.</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>

</html>